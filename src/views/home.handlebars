
<h1>homeA</h1>
<div class="contenedorProductos">
    {{#each products}}
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">{{this.id}}</h5>
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">{{this.price}}</p>
            <p class="card-text">{{this.description}}</p>
            <p class="card-text">{{this.stock}}</p>
            <p class="card-text">{{this.status}}</p>
            <p class="card-text">{{this.category}}</p>
            <p class="card-text">{{this.code}}</p>
            <p class="card-text">{{this.thumbnails}}</p>
            
        </div>
    </div>
    {{/each}}
    
</div>